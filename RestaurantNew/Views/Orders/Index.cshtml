@model IEnumerable<RestaurantNew.Models.Order>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Menu.NameDose)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.User.CustName)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.DateForDay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Count)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Menu.NameDose)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.User.CustName)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.DateForDay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Count)
        </td>
        <td>
           

            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
